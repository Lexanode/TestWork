generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum StatusReport {
  PENDING
  FAILED
  SUCCESS
}

model Report {
  id            Int          @id @default(autoincrement())
  status        StatusReport @default(PENDING)
  titleService  String
  columnHeaders String[]
  dataUrl       String
  excelFileUrl  String?
  errorMessage  String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
